{
  "editor.formatOnSave": true,
  "prettier.singleQuote": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.tslint": true,
    "source.organizeImports": true
  },
  "cSpell.words": [
    "Entypo",
    "Formik",
    "Moti",
    "Pressable",
    "Raleway",
    "Searchbar"
  ]
}
